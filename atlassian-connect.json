{
    "key": "vinnybot",
    "name": "VinnyBot",
    "description": "VinnyBot",
    "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}/atlassian-connect.json"
    },
    "capabilities": {
        "hipchatApiConsumer": {
            "scopes": [
                "send_notification"
            ]
        },
        "installable": {
            "callbackUrl": "{{localBaseUrl}}/installable"
        },
        "configurable": {
             "url": "{{localBaseUrl}}/config"
        },
        "webhook": {
            "url": "{{localBaseUrl}}/webhook",
            "pattern": "^/ping",
            "event": "room_message",
            "name": "Ping Pong"
        }
    }
}
